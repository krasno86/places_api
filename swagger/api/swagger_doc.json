{
  "swagger": "2.0",
  "info": {
    "title": "API documentation API V1"
  },
  "basePath": "/",
  "paths": {
    "/signup": {
      "post": {
        "summary": "Create new user",
        "tags": [
          "Registration"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "params",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "user": {
                  "type": "object",
                  "properties": {
                    "full_name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    },
                    "role": {
                      "type": "string",
                      "enum": [
                        "white",
                        "red"
                      ]
                    }
                  },
                  "required": [
                    "username",
                    "email",
                    "password"
                  ]
                }
              },
              "required": [
                "user"
              ]
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Signup"
          },
          "422": {
            "description": "invalid request"
          }
        }
      }
    }
  }
}